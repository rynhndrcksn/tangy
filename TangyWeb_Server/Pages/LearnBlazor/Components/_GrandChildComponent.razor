<div class="row">
    <h4>Grand Child Component</h4>
    <p>There is a message: @MessageForGrandChild</p>
    <p>Lucky number: @LuckyNumForGrandChild</p>
</div>
@code {
    [CascadingParameter(Name = "message")]
    public string MessageForGrandChild { get; set; }

    [CascadingParameter(Name = "luckyNum")]
    public int LuckyNumForGrandChild { get; set; }
}